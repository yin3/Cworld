#include <stdio.h>
#include <stdlib.h>
int a[100]; 
void sort(int a[],int l,int r)
{
	if(l>=r)
	  return ;
	int t=a[(l+r)/2],j=l-1,n=r+1;
	int x;
	while(j<n)
	{
		do ++j;while(a[j]<t);
		do --n;while(a[n]>t);
		if(j<n)
		{
			x=a[j];
			a[j]=a[n];
			a[n]=x;
		}
	}
	sort(a,0,j-1);
	sort(a,n+1,r);
}
int main( )
{
	int n,i;
	
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",a+i);
	}
	sort(a,0,n-1);
	for(i=0;i<n;i++)
	  printf("%3d",a[i]);
	return 0;
}
