public class Study10 {
    public static void main(String[] args){
        int[] a={1,4,3,2,5};
        int i;
        int n=a.length-1;
        while(n-->=0)
        for(i=0;i<a.length-1;i++)
            if(a[i]<a[i+1])
            {
                int t=a[i+1];
                a[i+1]=a[i];
                a[i]=t;
            }
        for(int c:a){
            System.out.print(c+" ");
        }

    }
    class Person{

    }
}
